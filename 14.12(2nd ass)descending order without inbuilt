#arrrangingin elements in descending order
n=int(input())
l=list(map(int,input().split()))
print(l)
"""for i in range(len(l)):
    print(l[i],end=" ")"""
for i in range(0,n-1):
    for j in range(i+1,n):
        if l[i]<l[j]:
            t=l[j]
            l[j]=l[i]
            l[i]=t
#print("elements in descending order are")
for i in range(len(l)):
      print("the numbers after arranging in descending order ",l[i],end=" ")
