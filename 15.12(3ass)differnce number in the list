#differnce number in the list
n=int(input())
l=list(map(int,input().split()))
print(l)
ev=0
target=int(input("required value is "))
for i in range(0,n):
    for j in range(i+1,n):
        if(l[i]-l[j]==target):
           ev=1
           break
if(ev==1):
    print("yes")
else:
    print("no")







